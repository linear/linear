{
  "name": "@linear/cli",
  "description": "A cli tool to interface with linear",
  "version": "0.1.0",
  "author": "Justin Bennett @zephraph",
  "bin": {
    "linear": "./lib/index.js"
  },
  "bugs": "https://github.com/linearapp/linear/issues",
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib"
  ],
  "homepage": "https://github.com/linearapp/linear",
  "keywords": [
    "linear",
    "cli"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "linearapp/linear",
  "scripts": {
    "start": "dotenv ./lib/index.js",
    "lint": "tslint -p .",
    "patch-package": "patch-package",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "dev": "tsc -b -w"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@linear/sdk": "^0.1.0",
    "await-to-js": "^2.1.1",
    "commander": "^3.0.2",
    "dedent": "^0.7.0",
    "prompts": "^2.2.1",
    "require-all": "^3.0.0",
    "tagged-template-noop": "^2.1.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@types/dedent": "^0.7.0",
    "@types/node": "^10",
    "@types/prompts": "^2.0.1",
    "dotenv-cli": "^2.0.1",
    "graphql": "^14.5.4",
    "jest": "^24.9.0",
    "patch-package": "^6.2.0",
    "ts-node": "^8"
  }
}
