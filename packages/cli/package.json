{
  "name": "@linear/cli",
  "description": "A cli tool to interface with linear",
  "version": "0.1.0",
  "author": "Justin Bennett @zephraph",
  "bin": {
    "linear": "./bin/run"
  },
  "bugs": "https://github.com/linearapp/linear/issues",
  "dependencies": {
    "@linear/sdk": "^0.1.0",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "await-to-js": "^2.1.1",
    "dedent": "^0.7.0",
    "graphql-request": "^1.8.2",
    "prompts": "^2.2.1",
    "tagged-template-noop": "^2.1.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.7.0",
    "@graphql-codegen/near-operation-file-preset": "^1.7.0",
    "@graphql-codegen/typescript": "^1.7.0",
    "@graphql-codegen/typescript-operations": "^1.7.0",
    "@oclif/dev-cli": "^1",
    "@types/dedent": "^0.7.0",
    "@types/node": "^10",
    "@types/prompts": "^2.0.1",
    "dotenv-cli": "^2.0.1",
    "globby": "^10",
    "graphql": "^14.5.4",
    "ts-node": "^8"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/linearapp/linear",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "linear",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "linearapp/linear",
  "scripts": {
    "start": "dotenv ./bin/run",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "generate": "graphql-codegen",
    "dev": "tsc -b -w"
  },
  "types": "lib/index.d.ts"
}
