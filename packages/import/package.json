{
  "name": "@linear/import",
  "description": "Import helpers for uploading issues to Linear",
  "version": "3.0.7",
  "author": "Linear Orbit, Inc",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json"
  },
  "bin": {
    "linear-import": "bin/linear-import.cjs"
  },
  "repository": "https://github.com/linear/linear",
  "engines": {
    "node": ">=18.x"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "cli": "ts-node src/cli",
    "cli:local": "NODE_TLS_REJECT_UNAUTHORIZED=0 ts-node src/cli --apiUrl https://local.linear.dev:8090/graphql",
    "dev:import": "NODE_ENV=development pnpm run build:import",
    "build:import": "tsdown --clean --format cjs src/cli.ts",
    "build:types": "tsc --noEmit"
  },
  "dependencies": {
    "@linear/sdk": "workspace:*",
    "chalk": "4.1.2",
    "cli-progress": "^3.12.0",
    "csvtojson": "2.0.10",
    "date-fns": "^2.19.0",
    "inquirer": "7.3.3",
    "inquirer-file-path": "1.0.1",
    "jira2md": "2.0.4",
    "lodash": "4.17.21",
    "node-fetch": "2.6.1",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.5",
    "@types/inquirer": "^7.3.3",
    "@types/node-fetch": "^2.5.7",
    "tsdown": "catalog:",
    "ts-node": "catalog:",
    "typescript": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "ac77c2c674a4266bd263b18213354572d3e8f9eb"
}
