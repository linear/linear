  },
  "peerDependencies": {
    "graphql": "^15.4.0",
    "graphql-request": "^3.3.0"
  },
  "devDependencies": {
    "@linear/codegen-test": "^1.0.2",
    "@rollup/plugin-typescript": "^8.1.1",
    "dotenv": "^8.2.0",
    "execa": "^5.0.0",
    "get-port": "^5.1.1",
    "rollup": "^2.38.3",
    "rollup-plugin-gzip": "^2.5.0",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^9.1.1"
